<div class="slds-grid slds-grid--vertical-stretch">
    <div class="slds-col slds-size--1-of-6">
        {{> adminnav }}
    </div>
    <div class="slds-col slds-size--5-of-6">
        
        <form name="youtubeSettingsForm" action="/admin/youtube" method="post" enctype="multipart/form-data">
            <h3 class="slds-text-heading--medium">Youtube Settings</h3>
            <div class="slds-form--stacked">
                <div class="slds-form-element">
                    <label class="slds-form-element__label" for="youtubeAPIKey">Youtube API Key</label>
                    <input type="text" class="slds-input" name="youtubeAPIKey" value="{{config.youtube.apiKey}}" placeholder="Enter Youtube API Key"/>
                </div>
                <div class="slds-form-element">
                    <button type="submit" class="slds-button slds-button--brand">Save</button>
                </div>
            </div>
        </form>
     
    </div>
</div>